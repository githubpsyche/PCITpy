    "#exports\n",
    "from scipy import stats\n",
    "\n",
    "\n",
    "def likratiotest(l1, l0, k1, k0):\n",
    "    D = -2 * (l0 - l1)  # deviance score\n",
    "    df = k1 - k0  # degrees of freedom\n",
    "    p = stats.chi2.sf(D, df)  # chi-square test\n",
    "    return D, p"
   ]
  }
